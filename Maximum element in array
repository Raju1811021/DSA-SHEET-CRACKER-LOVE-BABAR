#include<iostream>
using namespace std;
int maxElement(int *arr,int length)
{
    int max=arr[0];
    for(int i=0;i<length;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
        }
    }
    return(max);
}

int main()
{
    int arr[5]={10,8,9,7,110};
    cout<<maxElement(arr,5);
    return 0;
}
